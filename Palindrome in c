#include<stdio.h>

int main(){
int a = 0, b = 0, c = 0;

printf("Enter the num???\n");
scanf("%d",&a);
c = a;
while(c != 0){

b = b*10;
b = b + (c % 10);
c = c/10;

}
printf("\nthe result: %d",b);

if (a == b){
    printf("\npalindrome number");
}
else {
    printf("\nNot a palindrome number");
}
return 0;

}

